-- title:   Set palette color
-- author:  congusbongus, TIC-80 wiki
-- desc:    Set palette color to another
-- license: MIT License
-- script:  moon
-- tags:    draw palette swap color

t=0
x=96
y=24

-- set c0 to be c1 color, call pal() to reset
palset=(c0,c1)->
	if(c0==nil and c1==nil)
		for i=0,15
			poke4(0x3FF0*2+i,i)
	else
		poke4(0x3FF0*2+c0,c1)

export TIC=->
	if btn 0
		y-=1
	if btn 1
		y+=1
	if btn 2
		x-=1
	if btn 3
		x+=1
	if btnp(4)
		-- swap colors 0 and 12
		palset(12,0)
		palset(0,12)
	elseif btnp(5)
		palset!

	cls 13
	-- Draw palette squares
	for i=0,7
		rect(10+i*10,10,10,10,i)
	for i=8,15
		rect(10+(i-8)*10,20,10,10,i)
	spr 1+(t%60)//30*2,x,y,14,3,0,0,2,2
	print "Press A/B to swap palette colors!",32,84
	t+=1

