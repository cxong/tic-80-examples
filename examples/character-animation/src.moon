aspr=(
  id,x,y,colorkey=-1,sx=1,sy=1,flip=0,rotate=0,w=1,h=1,
  ox=w*8//2,oy=h*8//2,shx1=0,shy1=0,shx2=0,shy2=0
)->
  -- Draw a sprite using two textured triangles.
  -- Apply affine transformations: scale, shear, rotate, flip

  -- scale / flip
  if (flip%2)==1
    sx=-sx
  if flip>=2
    sy=-sy
  ox*=-sx
  oy*=-sy
  -- shear / rotate
  shx1*=-sx
  shy1*=-sy
  shx2*=-sx
  shy2*=-sy
  rot=(x, y, rotate)->
    sa = math.sin(math.rad(rotate))
    ca = math.cos(math.rad(rotate))
    return x * ca - y * sa, x * sa + y * ca
  rx1, ry1 = rot(ox + shx1, oy + shy1, rotate)
  rx2, ry2 = rot(((w * 8) * sx) + ox + shx1, oy + shy2, rotate)
  rx3, ry3 = rot(ox + shx2, ((h * 8) * sy) + oy + shy1, rotate)
  rx4, ry4 = rot(((w * 8) * sx) + ox + shx2, ((h * 8) * sy) + oy + shy2, rotate)
  x1 = x + rx1
  y1 = y + ry1
  x2 = x + rx2
  y2 = y + ry2
  x3 = x + rx3
  y3 = y + ry3
  x4 = x + rx4
  y4 = y + ry4
  -- UV coords
  u1=(id%16)*8
  v1=id//16*8
  u2=u1+w*8
  v2=v1+h*8

  ttri(x1,y1,x2,y2,x3,y3,u1,v1,u2,v1,u1,v2,false,colorkey)
  ttri(x3,y3,x4,y4,x2,y2,u1,v2,u2,v2,u2,v1,false,colorkey)

FONTW=6

class ToggleButton
  new:(x,y,sprites,label,colorkey,textcolor)=>
    @x=x
    @y=y
    @sprites=sprites
    @label=label
    @colorkey=colorkey
    @textcolor=textcolor
    @width=string.len(label)*FONTW+8
    @height=8
    @wasDown=false
    @value=false

  update:=>
    mx,my,left=mouse!
    hover=mx>=@x and mx<=@x+@width and my>=@y and my<=@y+@height
    -- Change cursor: hand
    if hover
      poke(0x3FFB,129)
    -- Clicking on press
    if left and hover and not @wasDown
      @value=not @value
    @wasDown=left

  draw:=>
    print @label,@x+10,@y+2,@textcolor
    sprite=@sprites[1]
    if @value
      sprite=@sprites[2]
    spr sprite,@x,@y,@colorkey

WIDTH=16
GROUND=120
JUMP=-5
GRAVITY=0.2
JUMPANTIC=20
TEXTCOLOR=12

class Hat
  new:=>
    @x=24
    @y=GROUND
    @dy=0
    @tx=0
    @ty=0
    @tdx=0
    @tdy=0

  update:(_x,_y,_dx,_dy,_sy,_ticcyid,overlapping)=>
    @tx=_x
    @ty=_y-_sy*32+32
    @tdx=_dx
    @tdy=_dy
    if @tdx<0
      @tx-=6
    if _ticcyid==1 or _ticcyid==5
      @ty+=2
    if overlapping
      @y+=@dy
      -- hat must not go below ticcy
      if @y>@ty
        @dy=(@ty-@y)*0.4
        @y=@ty
      ddy=@ty-@y
      if math.abs(ddy)>0.5
        ddy=ddy/math.abs(ddy)*0.5
      @dy+=ddy*0.3
      if math.abs(@dy)>3
        @dy=@dy/math.abs(@dy)*3
    else
      @y=@ty
      @dy=0
    @x=@tx

  draw:=>
    x=@x-4
    y=@y-48
    spr 272,x,y,0,2

easeOutElastic=(x)->
  if x==0
    return 0
  if x==1
    return 1
  c4=(2*math.pi)/3
  return math.pow(2,-10*x)*math.sin((x*10-0.75)*c4)+1

t=0
x=24
y=96
speed=1.0
dx=1
dy=0
flip=1
jumpCooldown=0
poseCooldown=0
wobbleCooldown=0
WOBBLE_COUNT=60
hat=Hat()
toggleSquash=ToggleButton(4,4,{256,257},"Squash/stretch",0,TEXTCOLOR)
toggleAntic=ToggleButton(4,16,{256,257},"Antic",0,TEXTCOLOR)
toggleStaging=ToggleButton(4,28,{256,257},"Staging",0,TEXTCOLOR)
toggleOverlap=ToggleButton(4,40,{256,257},"Overlapping action",0,TEXTCOLOR)
toggleSlow=ToggleButton(4,52,{256,257},"Slow in/out",0,TEXTCOLOR)
toggleArc=ToggleButton(120,4,{256,257},"Arc",0,TEXTCOLOR)
toggleSecondary=ToggleButton(120,16,{256,257},"Secondary action",0,TEXTCOLOR)
toggleTiming=ToggleButton(120,28,{256,257},"Timing",0,TEXTCOLOR)
toggleExaggerate=ToggleButton(120,40,{256,257},"Exaggeration",0,TEXTCOLOR)
toggleAppeal=ToggleButton(120,52,{256,257},"Appeal",0,TEXTCOLOR)

export TIC=->
  cls 1

  sx,sy=1,1
  ox,oy=8,16
  shx1,shy1,shx2,shy2=0,0,0,0

  if jumpCooldown==0 and poseCooldown==0 and wobbleCooldown==0
    -- Run left/right
    x+=dx*speed
    if toggleSlow.value and y>=GROUND
      -- Accelerate
      speed=math.min(speed+0.01,2)
    else
      speed=1
    -- Reverse direction when reaching edges of screen
    if dx>0
      if x>240-WIDTH
        dx=-1
        flip=0
        if toggleSlow.value
          speed=0
    elseif dx<0
      if x<WIDTH
        dx=1
        flip=1
        if toggleSlow.value
          speed=0
  
  if btnp(4) and y>=GROUND
    if toggleAntic.value
      jumpCooldown=JUMPANTIC
    dy=JUMP
  elseif dy>0 and y>GROUND
    dy=0
    y=GROUND

  if jumpCooldown==0
    if toggleArc.value
      y+=math.abs(dy)*dy*0.31
    else
      y+=dy
    dy+=GRAVITY
  else
    jumpCooldown-=1
  if poseCooldown>0
    speed=0
    poseCooldown-=1
    if poseCooldown==0 and toggleExaggerate.value
      toggleExaggerate.value=false
  elseif toggleExaggerate.value
    poseCooldown=60
  if wobbleCooldown>0
    speed=0
    wobbleCooldown-=1
    if wobbleCooldown==0 and toggleTiming.value
      toggleTiming.value=false
      shx1=0
    else
      shx1=(1-easeOutElastic(1-wobbleCooldown/WOBBLE_COUNT))*8
  elseif toggleTiming.value
    wobbleCooldown=WOBBLE_COUNT

  frame=(t%40)//10
  id=1+frame*2
  fframe=(t%80)//70
  fid=97+(frame%2)*2+fframe*32
  if y<GROUND
    id=33
    fid=161+(frame%2)*2
    if toggleSquash.value
      -- squash down at top of jump arc
      sx=1.3-dy*dy*0.03
      sy=1/sx
  elseif jumpCooldown>0
    if toggleAntic.value
      -- squash down in anticipation of jump
      id=35
      fid=131
      sy=math.max(jumpCooldown/JUMPANTIC,0.5)
      sx=math.min(1/sy, 1.5)

  hat\update(x,y,dx,dy,sy,id,toggleOverlap.value)
  poke(0x3FFB,128)  -- cursor pointer
  toggleSquash\update!
  toggleAntic\update!
  toggleStaging\update!
  toggleOverlap\update!
  toggleSlow\update!
  toggleArc\update!
  toggleSecondary\update!
  toggleTiming\update!
  toggleExaggerate\update!
  toggleAppeal\update!

  if toggleStaging.value
    map!
  toggleSquash\draw!
  toggleAntic\draw!
  toggleStaging\draw!
  toggleOverlap\draw!
  toggleSlow\draw!
  toggleArc\draw!
  toggleSecondary\draw!
  toggleTiming\draw!
  toggleExaggerate\draw!
  toggleAppeal\draw!
  if poseCooldown>0
    aspr 193,x+(flip-0.5)*32,y,14,sx*2,sy*2,flip,rotate,4,2,ox,oy,shx1,shy1,shx2,shy2
  else
    aspr id,x,y,14,sx*2,sy*2,flip,rotate,2,2,ox,oy,shx1,shy1,shx2,shy2
    if toggleSecondary.value
      -- Draw facial expressions
      aspr fid,x,y,14,sx*2,sy*2,flip,rotate,2,2,ox,oy,shx1,shy1,shx2,shy2
    if toggleAppeal.value
      -- Draw cute eyes
      yy=y
      if id==1 or id==5
        yy+=2
      aspr 65,x,yy,14,sx*2,sy*2,flip,rotate,2,2,ox,oy,shx1,shy1,shx2,shy2
  hat\draw!

  print "Press Z to jump!",80,124,TEXTCOLOR

  t+=speed


