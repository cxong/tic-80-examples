-- script:  moon
-- tags:    collision sprite physics AABB

t=0
W=32
H=32
WIDTH=240
HEIGHT=136
SPEED=2

overlaps=(x1,y1,w1,h1,x2,y2,w2,h2)->
    -- Check if two AABBs overlap
    return x1+w1>x2 and x2+w2>x1 and y1+h1>y2 and y2+h2>y1

class Rect
    new:(x,y,w,h)=>
        @x=x
        @y=y
        @w=w
        @h=h

    draw:=>
        rect @x,@y,@w,@h,8


class Sprite extends Rect
    new:(x,y)=>
        super x,y,W,H

    move:(dx,dy,rects)=>
        -- Try to move to dx,dy; if it fails try to move to dx,0 or 0,dy
        if @\ismoveok(@x+dx,@y+dy,rects)
            @x+=dx
            @y+=dy
        elseif @\ismoveok(@x+dx,@y,rects)
            @x+=dx
        elseif @\ismoveok(@x,@y+dy,rects)
            @y+=dy

    ismoveok:(x,y,rects)=>
        -- Check if moving to x,y is ok
        -- Check edges of screen
        if x<0 or y<0 or x+@w>WIDTH or y+@h>HEIGHT
            return false
        -- Check overlap with all rects
        for rect in *rects
            if overlaps(x,y,@w,@h,rect.x,rect.y,rect.w,rect.h)
                return false
        return true

    draw:=>
        spr 1,@x,@y,14,2,0,0,2,2

rects={
    Rect(40,40,50,60),
    Rect(160,30,40,30),
    Rect(150,110,90,70)
}
sprite=Sprite(100,60)

export TIC=->
	cls 13
    dx=0
    dy=0
    if btn(0)
        dy=-SPEED
    if btn(1)
        dy=SPEED
    if btn(2)
        dx=-SPEED
    if btn(3)
        dx=SPEED
    sprite\move(dx,dy,rects)
    for rect in *rects
        rect\draw!
    sprite\draw!

