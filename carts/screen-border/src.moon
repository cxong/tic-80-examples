-- title:   Screen border
-- author:  congusbongus
-- desc:    Change the color of the screen border
-- license: MIT License
-- script:  moon
-- tags:    draw screen effect

MUSTEMPO=150
MUSSPD=3
FPS=60
MUSBEATTICKS=FPS*60/MUSTEMPO*MUSSPD/6
t=0
bg=13
textcolor=15
partymode=false

setborder=(c)->
    vbank(0)
    poke(0x03FF8,c)

export TIC=->
	if btnp(4) or btnp(5)
		partymode=not partymode
        if partymode
            music(0)
            bg=15
            textcolor=13
        else
            music(-1)
            setborder(0)
            t=0
            bg=13
            textcolor=15
            bgcounter=0

	cls bg
    spr (if partymode then 16 else 0),162,82,14,3
	spr (if partymode then 3 else 1),120,70,14,3,0,0,2,2
	print("PRESS A/B TO PARTY!",60,50,textcolor)
    if partymode
        t+=1
        if t==MUSBEATTICKS
            t=0
            setborder(math.random(0,15))
            bg=if bg==13 then 15 else 13
            textcolor=if textcolor==13 then 15 else 13

