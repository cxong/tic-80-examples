-- title:   Sky Gradient
-- author:  congusbongus
-- desc:    Draw a horizontal gradient in the background
-- license: MIT License
-- script:  moon
-- tags:    draw gradient horizon sky background

start_row = 50
end_row = 130
start_r = 0
start_g = 0
start_b = 200
end_r = 180
end_g = 255
end_b = 255

export BDR=(scanline)->
    -- https://github.com/nesbox/TIC-80/wiki/BDR
    -- Set palette on eery scanline, modifying the first palette color,
    -- which is used as the background
    -- https://github.com/nesbox/TIC-80/wiki/RAM#palette
    if scanline <= start_row
        poke(0x3FC0,start_r)
        poke(0x3FC1,start_g)
        poke(0x3FC2,start_b)
    elseif scanline >= end_row
        poke(0x3FC0,end_r)
        poke(0x3FC1,end_g)
        poke(0x3FC2,end_b)
    else
        d = (scanline - start_row) / (end_row - start_row)
        poke(0x3FC0,start_r + (end_r - start_r) * d)
        poke(0x3FC1,start_g + (end_g - start_g) * d)
        poke(0x3FC2,start_b + (end_b - start_b) * d)


export TIC=->
    cls(0)


